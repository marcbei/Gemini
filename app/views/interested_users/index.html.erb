<h1>Listing interested_users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @interested_users.each do |interested_user| %>
  <tr>
    <td><%= interested_user.name %></td>
    <td><%= interested_user.email %></td>
    <td><%= link_to 'Show', interested_user %></td>
    <td><%= link_to 'Edit', edit_interested_user_path(interested_user) %></td>
    <td><%= link_to 'Destroy', interested_user, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Interested user', new_interested_user_path %>
